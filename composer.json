{
    "name": "tsdevelopment/doctrine-behaviors-hashidable",
    "description": "Add hashidable behaviour to your doctrine entities",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "behaviors",
        "doctrine",
        "hashidable"
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-bcmath": "*",
        "doctrine/doctrine-bundle": "^2.1",
        "roukmoute/hashids-bundle": "^2.3",
        "symfony/doctrine-bridge": "^4.4|^5.0|^5.1",
        "symfony/dotenv": "^4.4|^5.0|^5.1",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.4|^5.0|^5.1",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^4.4|^5.0|^5.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "TSDevelopment\\DoctrineBehaviorsHashidable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TSDevelopment\\DoctrineBehaviorsHashidable\\Tests\\": "tests/"
        }
    }
}
